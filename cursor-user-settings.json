{
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.useSplitJSON": true,
  "workbench.settings.editor": "json",
  "workbench.colorCustomizations": {
    "activityBar.background": "#000000"
  },
  "workbench.colorTheme": "Dark++ Italic",
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 1000,
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "editor.formatOnType": true,
  "editor.wordWrap": "on",
  "editor.inlineSuggest.enabled": true,
  "editor.wordWrapColumn": 80,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "json.schemas": [],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.bracketPairColorization.enabled": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.automationProfile.osx": {},
  "terminal.integrated.fontFamily": "SauceCodePro Nerd Font Mono,Source Code Pro",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.cursorBlinking": true,
  "debug.javascript.autoAttachFilter": "onlyWithFlag",
  "gitlens.hovers.currentLine.details": false,
  "gitlens.hovers.currentLine.changes": false,
  "gitlens.hovers.annotations.details": false,
  "gitlens.hovers.annotations.changes": false,
  "python.formatting.provider": "black",
  "python.formatting.blackArgs": ["--line-length", "120"],
  "python.sortImports.args": ["--profile", "black"],
  "[python]": {
    "editor.formatOnType": true,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },

  "gitlens.graph.layout": "editor",
  "python.terminal.activateEnvironment": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "hediet.vscode-drawio.resizeImages": null,
  "hediet.vscode-drawio.theme": "atlas",
  "redhat.telemetry.enabled": false,
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "[terraform-vars]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "amazonQ.telemetry": false,
  "workbench.startupEditor": "none",
  "editor.renderWhitespace": "none",
  "cursor.cpp.disabledLanguages": ["plaintext", "markdown", "scminput"],
  "aws.telemetry": false,
  "makefile.configureOnOpen": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "workbench.editor.editorActionsLocation": "titleBar",
  "amazonQ.allowFeatureDevelopmentToRunCodeAndTests": {},
  "breadcrumbs.enabled": false,
  "cursor.composer.shouldAllowCustomModes": true,
  "cursor.terminal.usePreviewBox": true,
  "terminal.integrated.profiles.osx": {
    "zsh": {
      "path": "/opt/homebrew/bin/zsh",
      "args": ["-l"]
    }
  },
  "terminal.integrated.automationProfile.osx": {
    "path": "/opt/homebrew/bin/zsh",
    "args": ["-l"]
  },
  "cursor.terminal.shell.osx": "/opt/homebrew/bin/zsh",
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "aws.cloudformation.telemetry.enabled": false,
  "claudeCode.preferredLocation": "panel"
}
